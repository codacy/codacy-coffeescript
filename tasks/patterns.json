{
  "name": "@coffeelint/cli",
  "patterns": {
    "arrow_spacing": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": 0,
          "description": "Number of spaces in empty objects",
          "name": "empty_object_spaces"
        },
        {
          "default": 0,
          "description": "Number of spaces",
          "name": "spaces"
        }
      ]
    },
    "braces_spacing": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "bracket_spacing": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "camel_case_classes": {
      "category": "CodeStyle",
      "level": "Error"
    },
    "coffeescript_error": {
      "category": "CodeStyle",
      "level": "Error"
    },
    "colon_assignment_spacing": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "cyclomatic_complexity": {
      "category": "ErrorProne",
      "level": "Error",
      "parameters": [
        {
          "default": 10,
          "description": "Threshold",
          "name": "value"
        }
      ]
    },
    "duplicate_key": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "empty_constructor_needs_parens": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "ensure_comprehensions": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "eol_last": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "indentation": {
      "category": "CodeStyle",
      "level": "Error",
      "parameters": [
        {
          "default": 2,
          "description": "Indentation value",
          "name": "value"
        }
      ]
    },
    "line_endings": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": "unix",
          "description": "Line ending type",
          "name": "value"
        }
      ]
    },
    "max_line_length": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": 80,
          "description": "Line length",
          "name": "value"
        },
        {
          "default": true,
          "description": "Enforce line limit for comments",
          "name": "limitComments"
        }
      ]
    },
    "missing_fat_arrows": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": false,
          "description": "Strict",
          "name": "is_strict"
        }
      ]
    },
    "missing_parseint_radix": {
    "category": "ErrorProne",
    "level": "Warning"
    },
    "newlines_after_classes": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": 3,
          "description": "Number of lines",
          "name": "value"
        }
      ]
    },
    "no_backticks": {
      "category": "ErrorProne",
      "level": "Error"
    },
    "no_debugger": {
      "category": "ErrorProne",
      "level": "Warning",
      "parameters": [
        {
          "default": false,
          "description": "Detect console statements",
          "name": "console"
        }
      ]
    },
    "no_empty_functions": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_empty_param_list": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_implicit_braces": {
      "category": "CodeStyle",
      "level": "Info",
      "parameters": [
        {
          "default": true,
          "description": "Strict",
          "name": "strict"
        }
      ]
    },
    "no_implicit_parens": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_interpolation_in_single_quotes": {
      "category": "CodeStyle",
      "level": "Warning"
    },
    "no_nested_string_interpolation": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_plusplus": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_private_function_fat_arrows": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_spaces": {
      "category": "CodeStyle",
      "level": "Warning"
    },
    "no_stand_alone_at": {
      "category": "ErrorProne",
      "level": "Info"
    },
    "no_tabs": {
      "category": "CodeStyle",
      "level": "Error"
    },
    "no_this": {
      "category": "ErrorProne",
      "level": "Warning"
    },
    "no_throwing_strings": {
      "category": "ErrorProne",
      "level": "Error"
    },
    "no_trailing_semicolons": {
      "category": "CodeStyle",
      "level": "Warning"
    },
    "no_trailing_whitespace": {
      "category": "CodeStyle",
      "level": "Warning",
      "parameters": [
        {
          "default": false,
          "description": "Allow in comments",
          "name": "allowed_in_comments"
        },
        {
          "default": true,
          "description": "Allow in empty lines",
          "name": "allowed_in_empty_lines"
        }
      ]
    },
    "no_unnecessary_double_quotes": {
      "category": "ErrorProne",
      "level": "Error"
    },
    "no_unnecessary_fat_arrows": {
      "category": "ErrorProne",
      "level": "Error"
    },
    "non_empty_constructor_needs_parens": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "object_shorthand": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "prefer_english_operator": {
      "category": "CodeStyle",
      "level": "Warning",
      "parameters": [
        {
          "default": "ignore",
          "description": "Disallow double ! operator",
          "name": "doubleNotLevel"
        }
      ]
    },
    "prefer_fat_arrows_in_methods": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "prefer_logical_operator": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "space_operators": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "spacing_after_comma": {
      "category": "CodeStyle",
      "level": "Info"
    },
    "transform_messes_up_line_numbers": {
      "category": "ErrorProne",
      "level": "Warning"
    }
  }
}
